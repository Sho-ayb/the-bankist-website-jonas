{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-unused-vars": "off",
    "no-console": "off",
    "func-names": "off",
    "no-debugger": "off",
    "prettier": ["error", { "singleQuote": true, "trailingComma": "none" }]
  }
}
